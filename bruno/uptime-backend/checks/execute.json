{
  "meta": {
    "name": "Execute Check",
    "type": "http"
  },
  "request": {
    "url": "{{baseUrl}}/api/checks/execute",
    "method": "POST",
    "headers": [
      {
        "name": "Authorization",
        "value": "Bearer {{authToken}}"
      },
      {
        "name": "Content-Type",
        "value": "application/json"
      }
    ],
    "body": {
      "mimeType": "application/json",
      "data": "{\n  \"monitorId\": {{monitorId}}\n}"
    }
  },
  "tests": [
    {
      "name": "Expect OK",
      "script": "client.test(\"Check executed\", () => {\n  client.assert(response.status === 200, \"Expected 200, got \" + response.status);\n});\n"
    }
  ]
}


