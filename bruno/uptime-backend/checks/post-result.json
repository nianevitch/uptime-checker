{
  "meta": {
    "name": "Post Check Result",
    "type": "http"
  },
  "request": {
    "url": "{{baseUrl}}/api/checks/result",
    "method": "POST",
    "headers": [
      {
        "name": "Authorization",
        "value": "Bearer {{authToken}}"
      },
      {
        "name": "Content-Type",
        "value": "application/json"
      }
    ],
    "body": {
      "mimeType": "application/json",
      "data": "{\n  \"monitorId\": {{monitorId}},\n  \"httpCode\": 200,\n  \"responseTimeMs\": 123.45,\n  \"errorMessage\": null\n}"
    }
  },
  "tests": [
    {
      "name": "Expect OK",
      "script": "client.test(\"Result recorded\", () => {\n  client.assert(response.status === 200, \"Expected 200, got \" + response.status);\n});\n"
    }
  ]
}


