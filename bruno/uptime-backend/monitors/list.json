{
  "meta": {
    "name": "List Monitors",
    "type": "http"
  },
  "request": {
    "url": "{{baseUrl}}/api/monitors",
    "method": "GET",
    "headers": [
      {
        "name": "Authorization",
        "value": "Bearer {{authToken}}"
      }
    ]
  },
  "tests": [
    {
      "name": "Expect OK",
      "script": "client.test(\"Fetched monitors\", () => {\n  client.assert(response.status === 200, \"Expected 200, got \" + response.status);\n});\n"
    }
  ]
}


