meta {
  name: Register
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/auth/register
  body: json
  auth: inherit
}

headers {
  mimeType: application/json
}

body:json {
  {
    "email": "{{registerEmail}}",
    "password": "{{registerPassword}}"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
